              [ 25, "https://pmn.plantcyc.org/reaction?orgid=WHEATD&id=RXN-4142", 97, 113, 139, 126, "<b>Reaction:</b> EC 1.2.1.3 <b>ID:</b> RXN-4142<br>a fatty aldehyde + NAD<sup>+</sup> + H<sub>2</sub>O  &rarr;  a fatty acid + NADH + 2 H<SUP>+</SUP>" ],
